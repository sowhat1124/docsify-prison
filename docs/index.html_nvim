<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>2022</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="Description">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">
  
    <!-- Theme: Simple Dark -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/docsify-themeable@0/dist/css/theme-simple-dark.css">

    <!-- begin// themes not use now -->
    <!--
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/docsify-themeable@0/dist/css/theme-simple.css" >
    -->
    <!-- end// themes not use now -->

    <!-- Custom theme styles -->
    <style>
    /*
       check here: <https://jhildenbiddle.github.io/docsify-themeable/#/customization?id=code>
    */
    /* Content ===============================================*/
    :root {
        --base-font-size       : 16px;
        --base-background-color: #121212;
        --base-color: #875f5f;
        
        
        /* Headings */
        --heading-margin: 0 0 0;
        
        --heading-h1-color: #00875f;
        --heading-h1-font-size: 1rem;
        --heading-h1-font-weight: 600;
        --heading-h1-margin: 0 0 0;
        
        --heading-h2-color: #00875f;
        --heading-h2-font-size: 1rem;
        --heading-h2-font-weight: 600;
        
        --heading-h3-color: #00875f;
        --heading-h3-font-size: 1rem;
        --heading-h3-font-weight: 600;
        
        --heading-h4-color: #00875f;
        --heading-h4-font-size: 1rem;
        --heading-h4-font-weight: 600;
        
        --heading-h5-color: #00875f;
        --heading-h5-font-size: 1rem;
        --heading-h5-font-weight: 600;
        
        --heading-h6-color: #00875f;
        --heading-h6-font-size: 1rem;
        --heading-h6-font-weight: 600;

        /* links */
        --link-color: darkorange;
        /* code */
        --code-inline-background: rgb(45,45,45);
        /* blokquote */
        --blockquote-background: rgb(25,25,25);
        /* table */
        --table-row-odd-background: rgb(25,25,25);
        --table-head-background:;
        --table-head-font-weight: 100;
        /*
        */
    }
    h1::before {content: "# ";}
    h2::before {content: "## ";}
    h3::before {content: "### ";}
    h4::before {content: "#### ";}
    h5::before {content: "##### ";}
    h6::before {content: "###### ";}

    strong {
        color: #875faf;
      /* color: #875faf; */
        font-style: bold;
        font-weight: 600;
    }
    em {
        color: #5f5f87;
        font-style: normal;
        font-weight: ;
    }
    strong em {/*强调的斜体*/
        font-style: bold;
        font-weight: 100;
        color: red;
    }
    u {
        color: red;
    }
    th {
        font-style: normal;
    }
    </style>

  </head>

  <body>
    <div id="app"></div>

    <!-- copy from offical -->
    <script src="//cdn.jsdelivr.net/npm/docsify-plugin-carbon@1"></script>
  
    <!-- Setting -->
    <script>
      window.$docsify = {
        name: '',
        repo: '',
        name: '2022',
        maxLevel: 2,
        // 加载 _sidebar.md
        loadSidebar: true,
        autoHeader: false,
        subMaxLevel: 4,
        //mergeNavbar: true,
        loadNavbar: true,
        topMargin: 90, // default: 0
        coverpage: false,
        // relativePath
        //relativePath: true,
        // plug-in
        // 01-字数统计
        count:{
          countable:true,
          fontsize:'0.9em',
          color:'rgb(90,90,90)',
          language:'chinese',
        },

        //底部添加一对按钮用于跳转到上一篇或下一篇
        pagination: {
          previousText: '上一章节',
          nextText: '下一章节',
          crossChapter: true,
          crossChapterText: true,
        },

        // 頁面滾回頂部按鈕
        scrollToTop: {
          auto: false,
          text: 'Top',	<!-- 文字顯示 -->
          right: 15,		<!-- 方位右邊 -->
          bottom: 15,		<!-- 方位下面 -->
          offset: 500
        }

              //finish
      };
    </script>

    <!-- Docsify v4 -->
    <!-- 
        <script src="//cdn.jsdelivr.net/npm/docsify@4"></script>
    -->
    <script src="//cdn.jsdelivr.net/npm/docsify/lib/docsify.min.js"></script>
  
    <!-- plug-in -->
    <!-- 01-字数统计 -->
    <script src="//unpkg.com/docsify-count/dist/countable.js"></script>
    <!-- copy from offical -->
    <script src="/lib/plugins/search.js"></script>
    <script src="/lib/plugins/front-matter.js"></script>
    <!-- code high light -->
    <script src="//cdn.jsdelivr.net/npm/prismjs/components/prism-bash.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/prismjs/components/prism-markdown.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/prismjs/components/prism-nginx.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/prismjs/components/prism-php.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/prismjs/components/prism-python.min.js"></script>
    <!-- footnote -->
    <script src="//cdn.jsdelivr.net/npm/@sy-records/docsify-footnotes/lib/index.min.js"></script>
    <!-- 底部添加一对按钮用于跳转到上一篇或下一篇 -->
    <script src="//unpkg.com/docsify-pagination/dist/docsify-pagination.min.js"></script>
    <!-- search -->
    <script src="//unpkg.com/docsify/lib/plugins/search.js"></script>
    <!-- 頁面滾回頂部按鈕 -->
    <!-- <script src="//unpkg.com/docsify-scroll-to-top/dist/docsify-scroll-to-top.min.js"></script> -->
    <!-- toc -->

    <!-- front-matter -->
    <script src="//cdn.jsdelivr.net/npm/docsify/lib/plugins/front-matter.min.js"></script>
    <!-- not use now 
    ---
    ---
    not use -->


    <!-- Done -->
    </script>
  <!-- ar -->
  </body>

</html>

